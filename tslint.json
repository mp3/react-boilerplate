{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "interface-name": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "prefer-const": true,
    "prettier": [
      true, {
        "singleQuote": true,
        "semi": false,
        "jsxBracketSameLine": false
      }
    ]
  }
}
