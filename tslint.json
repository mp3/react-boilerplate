{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "no-unused-variable": true,
    "prefer-const": true,
    "prettier": [
      true, {
        "singleQuote": true,
        "semi": false,
        "jsxBracketSameLine": false
      }
    ]
  }
}
